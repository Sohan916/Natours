#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sohana/Desktop/personal/Natours/node_modules/.pnpm/concat@1.0.3/node_modules/concat/bin/node_modules:/Users/sohana/Desktop/personal/Natours/node_modules/.pnpm/concat@1.0.3/node_modules/concat/node_modules:/Users/sohana/Desktop/personal/Natours/node_modules/.pnpm/concat@1.0.3/node_modules:/Users/sohana/Desktop/personal/Natours/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sohana/Desktop/personal/Natours/node_modules/.pnpm/concat@1.0.3/node_modules/concat/bin/node_modules:/Users/sohana/Desktop/personal/Natours/node_modules/.pnpm/concat@1.0.3/node_modules/concat/node_modules:/Users/sohana/Desktop/personal/Natours/node_modules/.pnpm/concat@1.0.3/node_modules:/Users/sohana/Desktop/personal/Natours/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/concat@1.0.3/node_modules/concat/bin/concat" "$@"
else
  exec node  "$basedir/../.pnpm/concat@1.0.3/node_modules/concat/bin/concat" "$@"
fi
